	<query language="SQL"><![CDATA[SELECT idTicketFK AS 'ID', date_format(fechaTicket, '%d/%m/%Y') AS 'FECHA', CONCAT(ROUND(SUM(precioArticulo * cantidadArticulo), 2), ' â‚¬') AS 'IMPORTE TOTAL' FROM detalles_tickets JOIN articulos ON detalles_tickets.idArticuloFK = articulos.idArticulo 
JOIN tickets ON detalles_tickets.idTicketFK = tickets.idTicket WHERE fechaTicket BETWEEN $P{fechaDesde} AND $P{fechaHasta} GROUP BY idTicketFK;]]></query>
